<AttributeDefinition id="spid_REMOTE_USER" xsi:type="ScriptedAttribute" customObjectRef="shibboleth.HttpServletRequest">
    <AttributeEncoder xsi:type="SAML2String" name="spid_REMOTE_USER" friendlyName="spid_REMOTE_USER"/>
    <Script><![CDATA[
                if (custom.getHeader("spid_REMOTE_USER") != ""  && custom.getHeader("spid_REMOTE_USER") != "(null)" ) {
                        spid_REMOTE_USER.addValue(custom.getHeader("spid_REMOTE_USER"));
                }  else {
                        spid_REMOTE_USER.addValue(null);
                }

        ]]></Script>
</AttributeDefinition>

